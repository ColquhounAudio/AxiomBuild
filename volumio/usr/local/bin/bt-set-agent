#!/usr/bin/expect -f

set prompt "#"

spawn bluetoothctl
expect -re $prompt
send "power on\r"
sleep 0.1
expect -re $prompt
send "discoverable on\r"
sleep 0.1
expect -re $prompt
send "agent NoInputNoOutput\r"
sleep 0.1
expect -re $prompt
send "default-agent\r"
sleep 0.1
expect -re $prompt
send "quit\r"
expect eof
